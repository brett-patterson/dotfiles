{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "version": 2,
  "final_space": true,
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "text",
          "style": "plain",
          "foreground": "246",
          "template": "{{ .HostName }}:"
        },
        {
          "type": "path",
          "style": "plain",
          "foreground": "33",
          "template": "{{ .Path }}",
          "properties": {
            "style": "mixed",
            "mapped_locations": {
              "/Users/brett.patterson": "~"
            }
          }
        },
        {
          "type": "git",
          "style": "plain",
          "foreground": "72",
          "template": " ({{ .HEAD }})",
          "properties": {
            "branch_icon": ""
          }
        },
        {
          "type": "text",
          "style": "plain",
          "template": " $"
        }
      ],
      "newline": true
    }
  ]
}
